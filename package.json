{
  "name": "consigliere",
  "version": "1.0.0",
  "description": "Multi account AWS trusted advisor dashboard",
  "main": "index.js",
  "scripts": {
    "lintjscs": "npm run lint && npm run jscs",
    "lint": "jshint src/index.js src/lib src/models",
    "jscs": "jscs src/index.js src/lib src/models",
    "test": "NODE_CONFIG_DIR='./src/config' gulp test",
    "start": "export NODE_CONFIG_DIR='./src/config' ; node_modules/forever/bin/forever start -a -l /var/log/consigliere/forever.log -o /var/log/consigliere/forever.out.log -e /var/log/consigliere/forever.err.log src/index.js",
    "refresh_cache": "NODE_CONFIG_DIR='./src/config' node src/tools/refresh_cache.js",
    "postinstall": "mkdir -p src/public/scripts; node_modules/browserify/bin/cmd.js src/react/app.js -t [ babelify --presets [ es2015 react ] ] | node_modules/uglify-js/bin/uglifyjs > src/public/scripts/bundled.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newscorpaus/consigliere.git"
  },
  "author": "Ravi Nair",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/newscorpaus/consigliere/issues"
  },
  "homepage": "https://github.com/newscorpaus/consigliere/wiki",
  "dependencies": {
    "async": "^1.5.0",
    "aws-sdk": "^2.2.19",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "casper": "^0.1.1",
    "config": "^1.16.0",
    "cron": "^1.0.9",
    "dynamoose": "^0.4.1",
    "forever": "^0.15.1",
    "gulp-bower": "0.0.10",
    "gulp-html-replace": "^1.5.5",
    "gulp-react": "^3.1.0",
    "gulp-streamify": "^1.0.2",
    "handlebars": "^4.0.4",
    "hapi": "^11.1.0",
    "history": "^1.17.0",
    "hoek": "^3.0.1",
    "inert": "^3.2.0",
    "jquery": "^2.1.4",
    "js-yaml": "^3.4.3",
    "lodash": "^4.16.2",
    "node-jsx": "^0.13.3",
    "path": "^0.12.7",
    "proxy-agent": "^2.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "reactify": "^1.1.1",
    "redis": "^2.4.2",
    "spin": "0.0.1",
    "sqlite3": "^3.1.1",
    "uglify-js": "^2.4.10",
    "vision": "^4.0.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-esnext": "^4.1.0",
    "gulp-jshint": "^2.0.1",
    "gulp-mocha": "^2.1.3",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.4.2",
    "install": "^0.3.0",
    "jscs": "^3.0.7",
    "jshint": "^2.9.4",
    "less-mkdirp": "^1.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.3",
    "npm": "^3.4.0",
    "sinon": "^1.17.6",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.1"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
